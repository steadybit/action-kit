{
  "$id": "https://schema.steadybit.io/attack-kit/messages",
  "title": "Messages",
  "type": "array",
  "description": "Log-messages that will be passed to the agent log.",
  "items": {
    "type": "object",
    "properties": {
      "message": {
        "type": "string"
      },
      "level": {
        "type": "string",
        "enum": [
          "debug",
          "info",
          "error"
        ],
        "default": "info"
      }
    },
    "required": [
      "message"
    ]
  }
}